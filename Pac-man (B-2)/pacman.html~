<script>
var score=0;
var dotid=[];
var ghostid=[];
var ghost2id=[];
var i=1;
var j=1;
var dotv=[];
var right=[];
var left=[];
var up=[];
var down=[];
var k=0;
var m=2;
var n=8;
var x=2;
var y=7;

function idloading()
{
document.getElementById("play").style.color="green";
for(var a=1;a<6;a++)
{
	dotid[a]=[];
	ghostid[a]=[];
	ghost2id[a]=[];
	dotv[a]=[];
	up[a]=[];
	down[a]=[];
	right[a]=[];
	left[a]=[];
	for(var b=1; b<13; b++)
	{
				

		if(a==3&&b>4&&b<9)
			continue;
		dotid[a][b]=a*100+b;
		ghostid[a][b]=a*100+b;
		ghost2id[a][b]=a*100+b;
		dotv[a][b]=0;
		if(((a==1)&&(b==3||b==9))||((a==2)&&(b==1||b==11))||((a==3)&&b==4)||((a==4)&&((b==1)||b==11))||((a==5)&&(b==3||b==9)))
		    right[a][b]=1;
		else
		    right[a][b]=0;
		if(((a==1)&&(b==4||b==10))||((a==2)&&(b==2||b==12))||((a==3)&&b==9)||((a==4)&&((b==2)||b==12))||((a==5)&&(b==4||b==10)))
		    left[a][b]=1;
		else
		left[a][b]=0;
		if((a==1&&(b==2||b==5||b==6||b==7||b==8||b==11))||(a==2&&(b==3||b==5||b==6||b==7||b==8||b==10))||(a==3&&(b==3||b==10))||(a==4&&(b==2||b==5||b==6||b==7||b==8||b==11)))
		down[a][b]=1;
		else
		down[a][b]=0;
		
		if((a==2&&(b==2||b==5||b==6||b==7||b==8||b==11))||(a==3&&(b==3||b==5||b==6||b==7||b==8||b==10))||(a==4&&(b==3||b==5||b==6||b==7||b==8||b==10))||(a==4&&(b==3||b==5||b==6||b==7||b==8||b==10))||(a==5&&(b==2||b==5||b==6||b==7||b==8||b==11)))
		up[a][b]=1;
		else
		up[a][b]=0;
		
		
		    
	}

}
}



function llist()
{	
	this.right=function(){
		if(check(1)==1)			//if check(x)==1 there is no wall.
	{
	dotv[i][j]=1;

	j++;
	}
	return dotid[i][j];
	}
	
	this.left=function(){
	
		if(check(2)==1)
	{
	dotv[i][j]=1;

	j--;
	}

	return dotid[i][j];
	}
	

	this.down=function(){
	if(check(4)==1)
	{
	dotv[i][j]=1;

	i++;
	}	
	return dotid[i][j];
	}

	this.up=function(){
		if(check(3)==1)
	{
	dotv[i][j]=1;

	i--;
	}

	return dotid[i][j];
	}

	this.current=function(){
		dotv[i][j]=1;
	return dotid[i][j];
	}
}

function glist()
{	
	this.right=function(){
		if(checkg(1)==1&&n!=12)
	{
		n++;
	}
	else
	 ghost.down();
	return ghostid[m][n];
	}
	
	this.left=function(){
	
	if(checkg(2)==1&&n!=1)
	{
		n--;
	}
	else 

	ghost.up();
	return ghostid[m][n];
	}
	

	this.down=function(){
	if(checkg(4)==1&&m!=5)
	{
		m++;
	}	
	else 
	ghost.right();
	return ghostid[m][n];
	}

	this.up=function(){
		if(checkg(3)==1&&m!=1)
	{
		m--;
	}
	else
	 ghost.left();
	return ghostid[m][n];
	}

	this.current=function(){
	return ghostid[m][n];
	}
}

function g2list()
{	
	this.right=function(){
		if(check2g(1)==1&&y!=12)
	{
		y++;
	}
	else
	 ghost2.up();
	return ghost2id[x][y];
	}
	
	this.left=function(){
	
	if(check2g(2)==1&&y!=1)
	{
		y--;
	}
	else 

	ghost2.down();
	return ghost2id[x][y];
	}
	

	this.down=function(){
	if(check2g(4)==1&&x!=5)
	{
		x++;
	}	
	else 
	ghost2.right();
	return ghost2id[x][y];
	}

	this.up=function(){
		if(check2g(3)==1&&x!=1)
	{
		x--;
	}
	else
	 ghost2.left();
	return ghost2id[x][y];
	}

	this.current=function(){
	return ghost2id[x][y];
	}
}

var dot=new llist();
var ghost=new glist();
var ghost2=new g2list();
var tar;
var gtar;
var g2tar;
function press(e)
{		tar=dot.current();
		gtar=ghost.current();
		g2tar=ghost2.current();
	 var event = window.event ? window.event : e;
    	
	var rnum=Math.ceil(Math.random()*4);
	var r2num=follow();
	
	if((i==m&&j==n)||(i==x&&j==y))
		{
		alert("GAME OVER");
		return;
		
		}
	if(score==275)
		{
		alert("You Won");
		return;
		
		}
	


	if(event.keyCode==37)
	{
	
	if(j!=1)
	{
	
	var ele=document.getElementById(tar);

		ele.src="./black.png";
		ele.style.width="5px";
		ele.style.height="5px";

	tar=dot.left();
	var ele=document.getElementById(tar);
		    if(dotv[i][j]==0)
		    score+=5;
	ele.src="./left.gif";
	ele.style.width="45px";
	ele.style.height="45px";
	
	}
	}
	if(event.keyCode==38)
		{
	
	if(i!=1)
	{

		var ele=document.getElementById(tar);
		ele.src="./black.png";
		ele.style.width="5px";
		ele.style.height="5px";
		tar=dot.up();
	var ele=document.getElementById(tar);
	if(dotv[i][j]==0)
		    score+=5;
	ele.src="./up.gif";
	ele.style.width="45px";
	ele.style.height="45px";
	
	}
		}
	if(event.keyCode==39)
		{
	if(j!=12)
	{
		var ele=document.getElementById(tar);

		ele.src="./black.png";
		ele.style.width="5px";
		ele.style.height="5px";
		tar=dot.right();
	var ele=document.getElementById(tar);
			//ele.style.visibility="visible";
			if(dotv[i][j]==0)
		    score+=5;
	ele.src="./right.gif";
	ele.style.width="45px";
	ele.style.height="45px";
	}
		}
	if(event.keyCode==40)
		{
	if(i!=5)
	{
		var ele=document.getElementById(tar);
		ele.src="./black.png";
		ele.style.width="5px";
		ele.style.height="5px";
		tar=dot.down();
	var ele=document.getElementById(tar);
	if(dotv[i][j]==0)
		    score+=5;
	ele.src="./down.gif";
	ele.style.width="45px";
	ele.style.height="45px";
	
	
	
	}
	
	}
	if(rnum==1)
		{
			if(n!=1)
			{			
			var gele=document.getElementById(gtar);
			if(dotv[m][n]==0)
			{gele.src="./dot.jpg";
			gele.style.width="15px";
			gele.style.height="15px";
			}
			else
			{
			gele.src="./black.png";
			gele.style.width="5px";
			gele.style.height="5px";
			}	
			gtar=ghost.left();
			var gele=document.getElementById(gtar);
			gele.src="./inky.svg";
			gele.style.width="45px";
			gele.style.height="45px";	


			}

		}	
	else if(rnum==2)
		{
			if(n!=12)
			{			
			var gele=document.getElementById(gtar);
			if(dotv[m][n]==0)
			{gele.src="./dot.jpg";
			gele.style.width="15px";
			gele.style.height="15px";
			}
			else
			{
			gele.src="./black.png";
			gele.style.width="5px";
			gele.style.height="5px";
			}	
			gtar=ghost.right();
			var gele=document.getElementById(gtar);
			gele.src="./inky.svg";
			gele.style.width="45px";
			gele.style.height="45px";	


			}


		}
	else if(rnum==3)
		{
			
			if(m!=1)
			{			
			var gele=document.getElementById(gtar);
			if(dotv[m][n]==0)
			{gele.src="./dot.jpg";
			gele.style.width="15px";
			gele.style.height="15px";
			}
			else
			{
			gele.src="./black.png";
			gele.style.width="5px";
			gele.style.height="5px";
			}	
			gtar=ghost.up();
			var gele=document.getElementById(gtar);
			gele.src="./inky.svg";
			gele.style.width="45px";
			gele.style.height="45px";	


			}


		}
	else if(rnum==4)
		{
			if(m!=5)
			{			
			var gele=document.getElementById(gtar);
			if(dotv[m][n]==0)
			{gele.src="./dot.jpg";
			gele.style.width="15px";
			gele.style.height="15px";
			}
			else
			{
			gele.src="./black.png";
			gele.style.width="5px";
			gele.style.height="5px";
			}

	
			gtar=ghost.down();
			
			var gele=document.getElementById(gtar);
			gele.src="./inky.svg";
			gele.style.width="45px";
			gele.style.height="45px";	


			}


		}
	
	/*sdfgsdgsdgsdgsdgsgsdgsdgsdgsd*/
	if(r2num==1)
		{
			if(y!=1)
			{			
			var gele=document.getElementById(g2tar);
			if(dotv[x][y]==0)
			{gele.src="./dot.jpg";
			gele.style.width="15px";
			gele.style.height="15px";
			}
			else
			{
			gele.src="./black.png";
			gele.style.width="5px";
			gele.style.height="5px";
			}	
