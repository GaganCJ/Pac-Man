		}
	
	
	
		
    scoref();
	
}
function scoref()
{
    document.getElementById("scre").innerHTML="SCORE :"+score;
}

function check(k)
{   
    if(k==1)
    {
    
    if(right[i][j]==1)
       	return 0;
    else 
	return 1;
    }
    if(k==2)
      {
    if(left[i][j]==1)
  	  return 0;
    else 
	return 1;
    }
    if(k==3)
    {
        if(up[i][j]==1)
           return 0;
        else
           return 1;
    }
     if(k==4)
    {
        if(down[i][j]==1)
           return 0;
        else
           return 1;
    }
}

function checkg(k)
{   
    if(k==1)
    {
    
    if(right[m][n]==1)
       	return 0;
    else 
	return 1;
    }
    if(k==2)
      {
    if(left[m][n]==1)
  	  return 0;
    else 
	return 1;
    }
    if(k==3)
    {
        if(up[m][n]==1)
           return 0;
        else
           return 1;
    }
     if(k==4)
    {
        if(down[m][n]==1)
           return 0;
        else
           return 1;
    }
}

function check2g(k)
{   
    if(k==1)
    {
    
    if(right[x][y]==1)
       	return 0;
    else 
	return 1;
    }
    if(k==2)
      {
    if(left[x][y]==1)
  	  return 0;
    else 
	return 1;
    }
    if(k==3)
    {
        if(up[x][y]==1)
           return 0;
        else
           return 1;
    }
     if(k==4)
    {
        if(down[x][y]==1)
           return 0;
        else
           return 1;
    }
}

function follow()
{	


	if(y>(j+1))
	return 1;
	else if(y<(j-1))
	return 2;
	else if(x>i)
	return 3;
	else return 4;



}

